	List<Student> s =  (List<Student>)request.getAttribute("studentList");
	for(int i = 0; i<s.size(); i++){ %>
		<h1>Roll Number : +${s.get(i).getRollNo()}</h1>
		<h1>Name : +${s.get(i).getName()}</h1>
		
<%	} %>
